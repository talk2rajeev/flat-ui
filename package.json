{
  "name": "sleek-ui",
  "version": "0.1.0",
  "description": "React UI Components, react UI kit",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "build": "rm -rf lib & tsc -p . && yarn copy-assets",
    "storybook": "start-storybook -p 6006",
    "dev:watchcss": "node watchAndMinifyCssFiles.js",
    "dev:storybook": "start-storybook -p 6006",
    "dev": "yarn init-css-minification & yarn dev:watchcss & yarn dev:storybook",
    "create-asset-within-lib": "mkdir lib/assets && mkdir lib/assets/css",
    "copy-assets": "yarn create-asset-within-lib && yarn copy-css",
    "copy-css": "cp -R ./src/assets/css/*.min.css ./lib/assets/css",
    "init-css-minification": "css-minify --dir src/assets/css --output src/assets/css",
    "build-storybook": "build-storybook"
  },
  "keywords": [
    "React UI Components",
    "React UI Kit",
    "react dropdown",
    "react radio",
    "react checkbox",
    "react accordian",
    "react menu",
    "react breadcrumb",
    "react toast",
    "react tooltip",
    "react card",
    "react table",
    "npm"
  ],
  "author": "Rajeev Sharma",
  "repository": "https://github.com/talk2rajeev/sleek-ui.git",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@types/react": ">=17.0.0",
    "babel-loader": "^8.2.3",
    "log-timestamp": "^0.3.0",
    "minify": "^8.0.3",
    "node-watch": "^0.7.2",
    "react": ">=17.0.0",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "@types/react": "^17.0.21",
    "react": "^17.0.2",
    "typescript": "^4.4.3"
  },
  "dependencies": {}
}
